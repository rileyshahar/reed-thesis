      \verb 10.1007/978-3-642-12821-9_2
      \endverb
      \verb{urlraw}
      \verb https://doi.org/10.1007/978-3-642-12821-9_2
      \endverb
      \verb{url}
      \verb https://doi.org/10.1007/978-3-642-12821-9_2
      \endverb
