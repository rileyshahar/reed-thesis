    \entry{simmons-2011}{book}{}
      \name{author}{1}{}{%
        {{hash=3b8d8037632d0039fa401a0c70a50348}{%
           family={Simmons},
           familyi={S\bibinitperiod},
           given={Harold},
           giveni={H\bibinitperiod}}}%
      }
